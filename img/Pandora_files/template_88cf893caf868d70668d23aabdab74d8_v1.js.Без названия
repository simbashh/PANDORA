$(document).ready(function(){

    $('.block-submenu__column.collection').on({
        mouseenter: function() {
            
            if(typeof $(this).data('picture') !== 'undefined') {
                $('.block-submenu__column_img[data-port="'+$(this).data('pictureport')+'"] img').attr('src',$(this).data('picture'));
            }
        },
        mouseleave: function() {
            $('.block-submenu__column_img.picture-box img').each((i,img)=>{
                var pic = $(img).data('picture');
                $(img).attr('src',pic);
            })
            
        }
    },'.list-submenu__item');

});
