$(document).ready(function() {
    /*
     * Галерея товаров
     */
    var galleryThumbs = SwiperProxy($('.gallery-thumbs'), {
        spaceBetween: 16,
        slidesPerView: 4,
        watchSlidesVisibility: true,
        watchSlidesProgress: true,
    });
    SwiperProxy($('.gallery-top'), {
        spaceBetween: 0,
        slidesPerView: 1,
        loop: true,
        nav: true,
        dots: false,
        thumbs: {
            swiper: galleryThumbs,
        },
    });

    $(document).on('click', '.product-gallery__tab', function (e) {
        e.preventDefault();

        $(this).addClass('is-active');
        $(this).siblings().removeClass('is-active');

        let id = $(this).data('id');
        let gal = document.getElementById(id);
        $(gal).addClass('active');
        $(gal).siblings().removeClass('active');
    });

    /**
     * Добавление товара в избранное
     */
    $(document).on("click", ".js-favorite", function (e) {
        e.preventDefault();
        let element = $(this),
            status = element.hasClass('favorite--active'),
            ID = element.attr('data-id'),
            url = '/local/ajax/actions/Favorite.php',
            action;

        if (status) {
            action = 'delete';
        } else {
            action = 'add';
        }

        $.ajax({
            url: url,
            type: 'post',
            data: {
                ID: ID,
                action: action
            },
            dataType: 'json',
            success: function (data) {
                if (data.status === 'success') {
                    updateFavoriteCounter();
                    if (status) {
                        element.removeClass('favorite--active');
                    } else {
                        element.addClass('favorite--active');
                        sendSimpleGaEvent('click_save_product');
                    }
                }
            }
        });
    });

    // клик на телефон и вне его
    $(document).on("click", '.phone-select__action', function () {
        $(this).toggleClass("phone-select__action_active");
        $(this).closest(".phone-select").find(".phone-select__list").toggleClass("phone-select__list_active");
    });

    $(document).mouseup(function (e) {
        var container = $(".phone-select__list");
        var arrow = $(".phone-select__action");
        if (e.target!==container[0] && !container.has(e.target).length && !arrow.has(e.target).length){
            $(container).removeClass("phone-select__list_active");
            $(arrow).removeClass("phone-select__action_active");
        }
    });

    // клик по селекту и вне его
    $(document).on("click", ".filter-select" , function() {
        $(this).closest(".js-open-select").find(".filter-select-container").toggleClass("filter-select-container--active")
    });

    $(document).mouseup(function (e) {
        var container = $(".filter-select-container");
        var box = $(".filter-select");
        if (e.target!==container[0] && !container.has(e.target).length && !box.has(e.target).length){
            $(container).removeClass("filter-select-container--active");
        }
    });

    $(".phonemask").mask('+7 000 000 00 00');

    $(document).on('click', '.search-action__close', function (e) {
        e.preventDefault();
        $('body').removeClass('panel-active');
        $('.search-panel').removeClass('is-active');
    });

    $(document).on('click', '.block-content-text__link', function (e) {
       e.preventDefault();
       $(this).parent().toggleClass('show_more');
    });

    // Переопределяем стандартный лоадер сайта
    BX.showWait = function () {
        var loader = $('.loader');
        $(loader).addClass('loader--active');
    };

    BX.closeWait = function () {
        var loader = $('.loader');
        $(loader).css({'visibility':'hidden','opacity': '0'});
    };

    updateFavoriteCounter();

    /**
     * Отслеживания DataLayer
     */
    $(document).on('click', '.js-goal-ig', function(e) {
        sendSimpleGaEvent('go_to_instagram');
    });
    $(document).on('click', '.js-goal-fb', function(e) {
        sendSimpleGaEvent('go_to_facebook');
    });
    $(document).on('click', '.js-goal-wa', function(e) {
        sendSimpleGaEvent('go_to_whatsapp');
    });
    $(document).on('click', '.js-goal-phone', function(e) {
        sendSimpleGaEvent('click_phone_number');
    });
    function sendSimpleGaEvent (value) {
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({'event': value});
    }

    /**
     * Электронная торговля
     */
    //Добавление товара в корзину из карточки товара.
    $(document).on('click', '.js-ga-add-to-card', function(e) {
        let ID = $(this).attr('data-id');
        if (ID) {
            $.ajax({
                url: '/local/ajax/analytics.php',
                type: 'post',
                data: {
                    ID: ID,
                    action: 'add'
                },
                dataType: 'json',
                success: function (data) {
                   if (data) {
                       window.dataLayer = window.dataLayer || [];
                       dataLayer.push({
                           'ecommerce': {
                               'currencyCode': 'KZT',
                               'add': {
                                   'products': [{
                                       'name': data.name,
                                       'id': data.id,
                                       'price': data.price,
                                       'brand': data.brand,
                                       'category': data.category,
                                       'quantity': data.quantity
                                   }]
                               }
                           },
                           'event': 'gtmUaEvent',
                           'gtmUaEventCategory': 'Enhanced Ecommerce',
                           'gtmUaEventAction': 'Adding a Product to a Shopping Cart',
                           'gtmUaEventNonInteraction': 'False'
                       });
                   }
                }
            });
        }
    });
    //Удаление товара из корзины
    $(document).on('click', '.js-ga-remove-from-card', function(e) {
        let ID = $(this).attr('data-product-id');
        if (ID) {
            $.ajax({
                url: '/local/ajax/analytics.php',
                type: 'post',
                data: {
                    ID: ID,
                    action: 'remove'
                },
                dataType: 'json',
                success: function (data) {
                    if (data) {
                        window.dataLayer = window.dataLayer || [];
                        dataLayer.push({
                            'ecommerce': {
                                'currencyCode': 'KZT',
                                'remove': {
                                    'products': [{
                                        'name': data.name,
                                        'id': data.id,
                                        'price': data.price,
                                        'brand': data.brand,
                                        'category': data.category,
                                        'quantity': data.quantity
                                    }]
                                }
                            },
                            'event': 'gtmUaEvent',
                            'gtmUaEventCategory': 'Enhanced Ecommerce',
                            'gtmUaEventAction': 'Removing a Product from a Shopping Cart',
                            'gtmUaEventNonInteraction': 'False'
                        });
                    }
                }
            });
        }
    });
    //Клик по товару в списке
    $(document).on('click', '.js-ga-product-click', function(e) {
        let ID = $(this).attr('data-id');
        if (ID) {
            $.ajax({
                url: '/local/ajax/analytics.php',
                type: 'post',
                data: {
                    ID: ID,
                    action: 'click'
                },
                dataType: 'json',
                success: function (data) {
                    if (data) {
                        window.dataLayer = window.dataLayer || [];
                        dataLayer.push({
                            'ecommerce': {
                                'currencyCode': 'KZT',
                                'click': {
                                    'actionField': {'list': list},
                                    'products': [{
                                        'name': data.name,
                                        'id': data.id,
                                        'price': data.price,
                                        'brand': data.brand,
                                        'category': data.category,
                                        'quantity': data.quantity,
                                        //'position': position
                                    }]
                                }
                            },
                            'event': 'gtmUaEvent',
                            'gtmUaEventCategory': 'Enhanced Ecommerce',
                            'gtmUaEventAction': 'Product Clicks',
                            'gtmUaEventNonInteraction': 'False'
                        });
                    }
                }
            });
        }
    });
    //Шаги оформления заказа - Шаг 1
    $(document).on('click', '.js-ga-step-1-click', function(e) {
        $.ajax({
            url: '/local/ajax/analytics.php',
            type: 'post',
            data: {
                action: 'order-step'
            },
            dataType: 'json',
            success: function (data) {
                if (data) {
                    window.dataLayer = window.dataLayer || [];
                    dataLayer.push({
                        'ecommerce': {
                            'currencyCode': 'KZT',
                            'checkout': {
                                'actionField': {'step': 1, 'option': ''},
                                'products': data.products
                            }
                        },
                        'event': 'gtmUaEvent',
                        'gtmUaEventCategory': 'Enhanced Ecommerce',
                        'gtmUaEventAction': 'Checkout Step 1',
                        'gtmUaEventNonInteraction': 'False'
                    });
                }
            }
        });
    });
    //Шаги оформления заказа - Шаг 6
    $(document).on('click', '.js-ga-step-2-click', function(e) {
        $.ajax({
            url: '/local/ajax/analytics.php',
            type: 'post',
            data: {
                action: 'order-step'
            },
            dataType: 'json',
            success: function (data) {
                if (data) {
                    window.dataLayer = window.dataLayer || [];
                    dataLayer.push({
                        'ecommerce': {
                            'currencyCode': 'KZT',
                            'checkout': {
                                'actionField': {'step': 2, 'option': ''},
                                'products': data.products
                            }
                        },
                        'event': 'gtmUaEvent',
                        'gtmUaEventCategory': 'Enhanced Ecommerce',
                        'gtmUaEventAction': 'Checkout Step 2',
                        'gtmUaEventNonInteraction': 'False'
                    });
                }
            }
        });
    });

    $(document).on('click', '.js-spoiler-more a', function (e) {
        e.preventDefault();
        let status = $('.js-spoiler').hasClass('js-spoiler--active'),
            height = $('.js-spoiler div .js-text').height();

        if (status) {
            $('.js-spoiler').removeClass('js-spoiler--active');
            $('.js-spoiler div:first').animate({maxHeight : '50px'}, 600);
            $(this).text('Больше');
        } else {
            $('.js-spoiler').addClass('js-spoiler--active');
            $('.js-spoiler div:first').animate({maxHeight : height}, 600);
            $(this).text('Меньше');
        }
    });

    $(document).on('click', '.js-open-specifications', function (e) {
        e.preventDefault();
        let height =  $('.specifications-list-b div:first').height();

        if ($(this).text() === "БОЛЬШЕ") {
            $('.specifications-list-b').animate({maxHeight : height}, 600);
        } else {
            $('.specifications-list-b').animate({maxHeight : '60px'}, 600);
        }

        $(this).text() === "БОЛЬШЕ" ? $(this).text("Меньше") : $(this).text("БОЛЬШЕ");
    });
    
    let inputs = document.querySelectorAll('.input__file');
    Array.prototype.forEach.call(inputs, function (input) {
        let label = input.nextElementSibling,
            labelVal = label.querySelector('.input__file-button-text').innerText;

        input.addEventListener('change', function (e) {
            let countFiles = '';
            if (this.files && this.files.length >= 1)
                countFiles = this.files.length;

            if (countFiles)
                label.querySelector('.input__file-button-text').innerText = 'Выбрано файлов: ' + countFiles;
            else
                label.querySelector('.input__file-button-text').innerText = labelVal;
        });
    });

    // phone Inputmask
    $(".phone_mask").inputmask('+7 (999) 999-99-99', {
        "onincomplete": function(){
            $('.phone_order').parent().find('.hidden_error').addClass('active');
            $('.phone_order').addClass('error_field');
        },
        "oncomplete": function(){
            let value = $('.phone_order').val();
            $('.phone_order').parent().find('.hidden_error').html('Это поле обязательно для заполнения.');
            $('.phone_order').parent().find('.hidden_error').removeClass('active');
            $('.phone_order').removeClass('error_field');

            if(parseInt(value.slice(4, 5)) !== 7) {
                $('.phone_order').parent().find('.hidden_error').html('Неправильный номер. Нужный формат: +7 (7xx) xxx-xx-xx');
                $('.phone_order').parent().find('.hidden_error').addClass('active');
                $('.phone_order').addClass('error_field');
            }
        }
    });

    $(".mask").inputmask('+7 (999) 999-99-99');
});

window.onbeforeunload = function(e) {
    //$('.loader').addClass('loader--active');
    return undefined;
};

function showNotification(id) {
    $.ajax({
        url: '/local/ajax/notification_show.php',
        type: 'post',
        data: {
            ID: id,
            action: 'show'
        },
        dataType: 'json',
        success: function (data) {
            //
        }
    });
}

updateFavoriteCounter = function () {
    $.ajax({
        url: '/local/ajax/actions/Favorite.php',
        type: 'post',
        data: {'action':'get'},
        dataType: 'json',
        success: function (data) {
            if (data.count > 0) {
                $('.favorite__value').remove();
                $('.header__like').append('<span class="favorite__value">'+data.count+'</span>');
            } else {
                $('.favorite__value').remove();
            }
        }
    });
};

/* End */
