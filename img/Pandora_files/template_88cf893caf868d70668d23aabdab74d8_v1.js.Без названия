let APP = window.APP || {};

APP.jqueryStyle = {

    init () {
        APP.jqueryStyle.sliders.init();
    },

    sliders: {
        sliders: [],
        options: {
            'into-home': {
                dots: true,
                nav: true,
                slidesPerView: 1,
                loop: true,
                autoplay: {
                    delay: 5000,
                    disableOnInteraction: false,
                },
            },
            'stocks': {
                dots: true,
                nav: true,
                slidesPerView: 1,
                spaceBetween: 30,
                loop: true
            }
        },
        init (node) {
            node = node || document;
            var slidersNodes = node.querySelectorAll('[data-slider]');
            var slider = slidersNodes.forEach((n)=>{
                var option = APP.jqueryStyle.sliders.options[n.dataset.slider] || {};
                n.removeAttribute('data-slider');
                SwiperProxy($(n), option);
            });

            console.log(slider);
        }
    }
    
};


window.addEventListener('load', ()=>{
    BX.ready(APP.jqueryStyle.init);
});




var iniTrash = function () {
    var $body = $('body');

    $("input[name='phone']").mask("+7 (000) 000-00-00");

    $(".modal-js").fancybox({
        animationEffect: "zoom-in-out",
        slideClass: "modal-close"
    });

    $("[data-fancybox]").fancybox({animationEffect: "zoom-in-out"});

    /*$(".phone-select__action").on("click", function () {
        $(this).toggleClass("phone-select__action_active"), $(this).closest(".phone-select").find(".phone-select__list").toggleClass("phone-select__list_active")
    });*/

    $(document).on("click", '.menu-change', function () {
        $(this).closest(".header").toggleClass("header_active").find(".menu-panel").toggleClass("menu-panel_active")
    });

    $(document).on("click", '[data-panel-trigger]', function (e) {
        e.preventDefault();
        $('[data-panel="' + $(this).data('panel-trigger') + '"]').addClass("is-active");
        $body.addClass("panel-active");
    })

    $(document).on("click", '[data-panel-close]', function (e) {
        e.preventDefault();
        $(this).closest('[data-panel]').removeClass("is-active");
        $body.removeClass("panel-active");
    });

    $(document).click(function (e) {
        var $target = $(e.target);

        if (
            $body.hasClass('panel-active') &&
            !$target.closest('[data-panel]').length &&
            !$target.is('[data-panel-trigger]') &&
            !$target.closest('[data-panel-trigger]').length
        ) {
            $('.panel.is-active').removeClass("is-active");
            $body.removeClass("panel-active");
        }
    });

    $(document).on("click", ".js-filter-open", function() {
        $(".filter-main").addClass("is-active");
        $("body").addClass("panel-active");
        return false
    });

    BX.addCustomEvent('onAjaxSuccess', function(){
        $(".js-list-basket-select").select2({minimumResultsForSearch: -1});
        $(".js-select").select2({minimumResultsForSearch: -1});
    });
    if (window.frameCacheVars !== undefined)
    {
        BX.addCustomEvent("onFrameDataReceived" , function(json) {
            $(".js-list-basket-select").select2({minimumResultsForSearch: -1});
            $(".js-select").select2({minimumResultsForSearch: -1});
        });
    } else {
        BX.ready(function() {
            $(".js-list-basket-select").select2({minimumResultsForSearch: -1});
            $(".js-select").select2({minimumResultsForSearch: -1});
        });
    }


    SwiperProxy($('.images_list'), {
        dots: true,
        nav: false,
        slidesPerView: 1,
        spaceBetween: 0,
        loop: true
    });

    SwiperProxy($('.js-list-advantages'), {
        dots: true,
        nav: true,
        slidesPerView: 1,
        loop: false,
        breakpoints: {
            767: {
                slidesPerView: 3
            }
        }
    });

    SwiperProxy($('.js-notifications-slider'), {
        dots: false,
        nav: false,
        slidesPerView: 1,
        loop: false
    });

    $(document).on("click", ".js-close-notifications", function() {
        $(".notifications").remove()
    });

    if(innerWidth > 992) {
        SwiperProxy($('.js-slider-card'), {
            dots: false,

            slidesPerView: "auto" ,
            breakpoints: {
                992: {
                    slidesPerView: 4 ,
                    spaceBetween: 2,
                    nav: true
                }
            },
            loop: false
        });
    }


    $(document).on("click", ".filter-category__name", function() {
        $(this).siblings(".filter-category__list").slideToggle();
        $(this).closest(".filter-category").find(".filter-category__name").toggleClass("filter-category__name--open");

    });

    $(document).on("click", ".dropdown-item__header", function() {
        $(this).siblings(".dropdown-item__body").slideToggle()
        $(this).closest(".dropdown-item").toggleClass("dropdown-item--open")
    });

    // $(".list-menu__item").on("click" , function() {
    //     $(".list-menu__item").removeClass("list-menu__item--active");
    //     $(this).addClass("list-menu__item--active")
    // });

    $(document).on("click", ".card-size__item", function() {
        $(".card-size__item").removeClass("card-size__item--active");
        $(this).addClass("card-size__item--active")
    });

    $(document).on("click", ".mobile-menu-catalog-name", function() {
        var attr = $(this).attr("data-target")
        $(".mobile-menu-catalog").removeClass("active");
        $(attr).addClass("active")
    });

    $(document).on("click", ".mobile-menu__item", function() {
        var attr = $(this).attr("data-target")
        if(attr) {
            event.preventDefault();
            $(attr).addClass("active")
        }
    });

    $(document).on("click", ".js-menu-mobile-open", function() {
        var attr = $(this).attr("data-target");
        if( $(".mobile-menu-catalog").hasClass("active")) {
            $(".mobile-menu-catalog").removeClass("active")
        }

        $(this).toggleClass("active");
        $(attr).toggleClass("active");
    });

    if(innerWidth < 767) {
        $(document).on("click", ".footer-nav__item-header", function() {
            $(this).siblings(".footer-nav__footer-menu").slideToggle();
            $(this).toggleClass("active")
        })
    }
    else {
        $(".basket-panel__list-basket-goods").mCustomScrollbar();

        $(".step-list-city").mCustomScrollbar();
    }
    if(innerWidth > 1024) {
        $(".shops-list").mCustomScrollbar();
    }
    $(document).on("click", ".view-list", function() {
        $(this).addClass("view--active");
        $(".view-table").removeClass("view--active")
        $(".list-goods--catalog").addClass("active");
    });

    $(document).on("click", ".view-table", function() {
        $(this).addClass("view--active");
        $(".view-list").removeClass("view--active")
        $(".list-goods--catalog").removeClass("active");
    });

    $(document).on("click", ".tab__item", function() {
        var id = $(this).attr("data-id")
        $(".tab__item").removeClass("tab__item--active");
        $(this).addClass("tab__item--active");

        $(".tabs-item").removeClass("tabs-item--active");
        $(".tabs-item#"+id).addClass("tabs-item--active");
    });

    $(document).on("click", ".site-message__open", function() {
        $(".site-message").addClass("site-message--active")
    });

    $(document).on("click", ".site-message__close", function() {
        $(".site-message").removeClass("site-message--active")
    });

    $(document).on("click", ".site-up", function(e) {
        $('body,html').animate({
            scrollTop: 0
        }, 1000);
        $(this).removeClass("active")
    });

    $("input[type=tel]").mask('+7 000 000 00 00');

    $(document).on("click", ".js-step-2", function(event) {
        event.preventDefault();
        $("#step-1").removeClass("step-item--active");
        $("#step-2").addClass("step-item--active");

        $(".step-list__item[data-step=2]").addClass("step-list__item--active")
    });

    $(document).on("click", ".js-step-3", function(event) {
        event.preventDefault();
        $("#step-2").removeClass("step-item--active");
        $("#step-3").addClass("step-item--active");

        $(".step-list__item[data-step=3]").addClass("step-list__item--active")
    });

    $(document).on("click", ".js-data-edits__add", function(event) {
        event.preventDefault();
        $(this).hide();
        $(this).siblings(".data-add-inputs").show()
    });

    $(document).on("click", ".tab-item", function(event) {
        var id = $(this).data("target");

        $(".tab-item").removeClass("tab-item--active");
        $(this).addClass("tab-item--active");
        if(id == "category-all") {
            return $("div[data-category]").show();
        }
        $("div[data-category]").hide();
        $("div[data-category="+id+"]").show();


    });

    $(document).on("click", ".js-lk-open", function(event) {
        $(".personal-info__panel").addClass("personal-info__panel--active")
        $(".personal-info").addClass("personal-info--active");
    });

    $(document).on("click", ".js-lk-close", function(event) {
        $(".personal-info__panel").removeClass("personal-info__panel--active")
        $(".personal-info").removeClass("personal-info--active")
    });

    $(document).on("click", ".js-open-date", function(event) {
        $(this).closest(".happy").addClass("happy--active");
    });

    $(document).on("click", ".js-open-date", function(event) {
        var el = $(this);
        var dest = el.attr('href'); // получаем направление
        if(dest !== undefined && dest !== '') { // проверяем существование
            $('html').animate({
                    scrollTop: $(dest).offset().top // прокручиваем страницу к требуемому элементу
                }, 500 // скорость прокрутки
            );

            $(".scrolls").removeClass("active")
            $(this).addClass("active")
        }
        return false;
    });


    function prepareMap (map) {
        ymaps.ready(function() {
            var _map = map;
            if(typeof map === "function") {
                var _map = map();
            }

            var container = $(_map.container.getParentElement()),
                buttons = $('<div class="controls_zoom"><div id="zoom-in"></div><div id="zoom-out"></div></div>');

            buttons.find("#zoom-in").on("click", function(){
                _map.setZoom(_map.getZoom() + 1, {checkZoomRange: true});
            });
            buttons.find("#zoom-out").on("click", function(){
                _map.setZoom(_map.getZoom() - 1, {checkZoomRange: true});
            });
            container.append(buttons);
        });
    };


    function maps() {

        myMap = new ymaps.Map('map',{
            center: [48.551736 , 66.904434] ,
            zoom: 4,
            controls: []
        });

        myCollection = new ymaps.GeoObjectCollection();

        if(typeof MAP_SHOPS !== "undefined") {
            if (MAP_SHOPS && MAP_SHOPS.length > 0) {
                for (var i in MAP_SHOPS) {
                    myPoints = [{
                        coords: [MAP_SHOPS[i].LAT, MAP_SHOPS[i].LON],
                        name: MAP_SHOPS[i].NAME
                    }];

                    var j = 0;

                    for (var k = 0, l = myPoints.length; k < l; k++) {
                        var point = myPoints[k];
                        myCollection.add(new ymaps.Placemark(point.coords,{
                            balloonContentBody: ['<span id="point_text">' + point.name + '</span>'].join('')
                        },{
                            iconLayout: "default#image",
                            iconImageHref: '/images/map.svg',
                            iconImageSize: [28, 42],
                            iconImageOffset: [-12, -21]
                        }));
                        j++
                    }
                    myMap.geoObjects.add(myCollection);
                }
            }
        }

        myMap.behaviors.disable('scrollZoom');

        if( innerWidth < 992) {
            myMap.behaviors.disable('drag');
        }
        return myMap;
    }

    function map_shop() {

        lat = $("#map-shop").data("lat");
        lon = $("#map-shop").data("lon");

        window.myMap = new ymaps.Map('map-shop', {
                center: [lat, lon],
                zoom: 15,
                scroll: false,
                controls: []
            }),
            myPlacemark = new ymaps.Placemark([lat, lon],{},{
                iconLayout: "default#image",
                iconImageHref: '/images/map.svg',
                iconImageSize: [28, 42],
                iconImageOffset: [-12, -21]
            });


        myMap.events.add(['balloonopen'], function(e) {
            myMap.balloon.close();
        });

        if( innerWidth < 992) {
            myMap.behaviors.disable('drag');
        }

        myMap.geoObjects.add(myPlacemark);
        myMap.behaviors.disable('scrollZoom');
        return myMap;
    }

    function shops_map() {
        var shops = $('.shops_map');
        ymaps.ready(function() {
            $(shops).each(function (i, el) {
                let id = $(el).attr('id');

                let lat = $("#"+id).data("lat");
                let lon = $("#"+id).data("lon");

                window.myMap = new ymaps.Map(id, {
                    center: [lat, lon],
                    zoom: 15,
                    scroll: false,
                    controls: []
                }),
                    myPlacemark = new ymaps.Placemark([lat, lon],{},{
                        iconLayout: "default#image",
                        iconImageHref: '/images/map.svg',
                        iconImageSize: [28, 42],
                        iconImageOffset: [-12, -21]
                    });


                myMap.events.add(['balloonopen'], function(e) {
                    myMap.balloon.close();
                });

                if( innerWidth < 992) {
                    myMap.behaviors.disable('drag');
                }

                myMap.geoObjects.add(myPlacemark);
                myMap.behaviors.disable('scrollZoom');


                let container = $(myMap.container.getParentElement()),
                    buttons = $('<div class="controls_zoom"><div id="zoom-in"></div><div id="zoom-out"></div></div>');

                var _map = myMap;
                buttons.find("#zoom-in").on("click", function(){
                    _map.setZoom(_map.getZoom() + 1, {checkZoomRange: true});
                });
                buttons.find("#zoom-out").on("click", function(){
                    _map.setZoom(_map.getZoom() - 1, {checkZoomRange: true});
                });
                container.append(buttons);
            });
        });
    }

    if ($("div").is("#map")) {
        prepareMap(maps);
    }
    if ($("div").is("#map-shop")) {
        prepareMap(map_shop);
    }
    shops_map();

    $(document).on("keyup", ".search-action__input", function(event) {
        if($(this).val().length > 1) {
            $(this).closest(".search-action").find(".search-action__clear").show();
        }
        else {
            $(this).closest(".search-action").find(".search-action__clear").hide();
        }
    });

    $(document).on("click", '.collapse-title', function() {
        $(this).closest(".collapse").toggleClass("collapse--active");
        $(this).siblings(".collapse-body").slideToggle();
    });
    function getTimeRemaining(endtime) {
        var t = Date.parse(endtime) - Date.parse(new Date());
        var seconds = Math.floor((t / 1000) % 60);
        var minutes = Math.floor((t / 1000 / 60) % 60);
        var hours = Math.floor((t / (1000 * 60 * 60)) % 24);
        var days = Math.floor(t / (1000 * 60 * 60 * 24));
        return {
            'total': t,
            'days': days,
            'hours': hours,
            'minutes': minutes,
            'seconds': seconds
        };
    }
    function initializeClock(id, endtime) {
        var clock = document.getElementById(id);
        var daysSpan = clock.querySelector('.days');
        var hoursSpan = clock.querySelector('.hours');
        var minutesSpan = clock.querySelector('.minutes');
        var secondsSpan = clock.querySelector('.seconds');

        function updateClock() {
            var t = getTimeRemaining(endtime);

            daysSpan.innerHTML = t.days;
            hoursSpan.innerHTML = ('0' + t.hours).slice(-2);
            minutesSpan.innerHTML = ('0' + t.minutes).slice(-2);
            secondsSpan.innerHTML = ('0' + t.seconds).slice(-2);

            if (t.total <= 0) {
                clearInterval(timeinterval);
            }
        }

        updateClock();
        var timeinterval = setInterval(updateClock, 1000);
    }
    if ($("div").is("#countdown")) {
        initializeClock('countdown', $("#countdown").attr("data-end"));
    }

    $(document).on("click", ".b-share", function(event) {
        event.preventDefault();
        $(this).toggleClass("b-share--active")
    });

    $(document).on("click", ".filter-select-container li" , function() {
        var name = $(this).text();
        $(this).closest(".filter-select-container").removeClass("filter-select-container--active");
        $(this).closest(".js-open-select").find(".filter-select span").text(name)
    });

    $(document).on("click", ".js-view-phone", function (e) {
        event.preventDefault();
        //$(this).hide();
        //$(".card-images__list").removeClass("card-images__list--hidden");
        $('.product-gallery').addClass('is-active');
    });



    var $range = $("#range");
    var $inputFrom = $("#range_form");
    var $inputTo = $("#range_to");
    var instance;
    var min = $("#range").data("min");
    var max = $("#range").data("max");
    var from = $("#range").data("from");
    var to = $("#range").data("to");

    $range.ionRangeSlider({
        skin: "round",
        type: "double",
        min: min,
        max: max,
        from: from,
        to: to,
        onStart: updateInputs,
        onChange: updateInputs,
        onFinish: updateInputs
    });
    instance = $range.data("ionRangeSlider");

    function updateInputs (data) {
        from = data.from;
        to = data.to;

        $inputFrom.prop("value", from);
        $inputTo.prop("value", to);
    }

    $inputFrom.on("keyup", function () {
        var val = $(this).prop("value");

        // validate
        if (val < min) {
            val = min;
        } else if (val > to) {
            val = to;
        }

        instance.update({
            from: val
        });

        $(this).prop("value", val);

    });

    $inputTo.on("keyup", function () {
        var val = $(this).prop("value");

        // validate
        if (val < from) {
            val = from;
        } else if (val > max) {
            val = max;
        }

        instance.update({
            to: val
        });

        $(this).prop("value", val);
    });


};

window.addEventListener('load', ()=>{
    BX.ready(() => {
            iniTrash();
        });
});

$(document).scroll(function(e) {
    if($(window).scrollTop() > 300) {
        $(".site-up").addClass("active")
    }
    else {
        $(".site-up").removeClass("active")

    }
})

function initializeClock(id, endtime) {
    var clock = document.getElementById(id);
    var daysSpan = clock.querySelector('.days');
    var hoursSpan = clock.querySelector('.hours');
    var minutesSpan = clock.querySelector('.minutes');
    var secondsSpan = clock.querySelector('.seconds');

    function updateClock() {
        var t = getTimeRemaining(endtime);

        daysSpan.innerHTML = t.days;
        hoursSpan.innerHTML = ('0' + t.hours).slice(-2);
        minutesSpan.innerHTML = ('0' + t.minutes).slice(-2);
        secondsSpan.innerHTML = ('0' + t.seconds).slice(-2);

        if (t.total <= 0) {
            clearInterval(timeinterval);
        }
    }

    updateClock();
    var timeinterval = setInterval(updateClock, 1000);
}

$(document).mouseup(function(e) {

    var share = $(".b-share");
    if (share.has(e.target).length === 0) {
        share.removeClass("b-share--active")
    }


});
