this.BX = this.BX || {};
(function (exports) {
    'use strict';

    var Translator = {
      languages: {
        ru: {
          name: 'Русский'
        },
        kz: {
          name: 'Қазақ тілі'
        }
      },
      "default": 'ru',
      bodyclassprefix: 'l18-',
      set: function set(lang) {
        var _this = this;
        if (lang && this.languages[lang]) ; else {
          lang = this.languages["default"];
        }
        if (window.BX && window.BX.setCookie) window.BX.setCookie('language', lang);
        document.querySelectorAll('body').forEach(function (b) {
          for (var Code in _this.languages) {
            if (Code == lang) {
              b.classList.add(_this.bodyclassprefix + Code);
            } else {
              b.classList.remove(_this.bodyclassprefix + Code);
            }
          }
          b.classList.remove();
        });
      }
    };

    exports.Translator = Translator;

}((this.BX.App = this.BX.App || {})));
